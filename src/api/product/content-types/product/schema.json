{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "KodeBarang": {
      "type": "string",
      "unique": true
    },
    "NamaBarang": {
      "type": "string",
      "required": true
    },
    "DeskripsiBarang": {
      "type": "blocks"
    },
    "StokBarang": {
      "type": "integer",
      "default": 0
    },
    "HargaBarang": {
      "type": "biginteger",
      "required": true
    },
    "HargaDiskon": {
      "type": "biginteger"
    },
    "BeratBarang": {
      "type": "decimal"
    },
    "GambarBarang": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "StatusBarang": {
      "type": "enumeration",
      "enum": [
        "Tersedia",
        "Habis",
        "Pre-order"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "supplier": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::supplier.supplier",
      "inversedBy": "products"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "product"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "product"
    }
  }
}
